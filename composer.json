{
  "name": "mtgtutor/console",
  "description": "CLI",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Pascal Kleindienst",
      "email": "pascal@mtg-tutor.de"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "lciolecki/php-image-optimizer": "dev-master",
    "intervention/image": "dev-master",
    "symfony/console": "^2.8",
    "fabpot/goutte": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "4.*",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3",
    "symfony/validator": "^2.8"
  },
  "autoload": {
    "psr-4": {
      "MtGTutor\\Console\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MtGTutor\\Console\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
